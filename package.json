    "@hookform/resolvers": "^5.0.1",
    "antd": "^5.24.9",
    "dayjs": "^1.11.13",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.3",
    "react-router": "^7.5.3",
    "yup": "^1.6.1",
    "zustand": "^5.0.3"
